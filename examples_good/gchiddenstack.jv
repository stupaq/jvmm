int main() {
  String hidden = "hidden OK";
  checkHidden();
  String r = checkReturn(100);
  printString(r);
  try {
    checkThrow(100);
  } catch (String e) {
    printString(e);
  }
  printString(hidden);
}

void checkHidden(int n) {
  if (n > 0)
    checkHidden(n - 1);
}

String checkReturn(int n) {
  if (n > 0)
    return checkReturn(n - 1);
  return "return OK";
}

void checkThrow(int n) throws String {
  if (n > 0)
    checkThrow(n - 1);
  else
    throw "throw OK";
}

